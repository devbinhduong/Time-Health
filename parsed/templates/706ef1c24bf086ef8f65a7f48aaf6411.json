{
  "components/common/quick-search": "<div class=\"quickSearch-form\">\n  <div class=\"container\">\n    {{> components/common/modal/modal-close-btn additional_attr='data-drop-down-close'}}\n    \n    <form class=\"form\" onsubmit=\"return false\" data-url=\"{{urls.search}}\" data-quick-search-form>\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"is-srOnly\" for=\"{{name}}\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input\"\n                       data-search-quick\n                       name=\"{{name}}\"\n                       id=\"{{name}}\"\n                       data-error-message=\"{{lang 'search.error.empty_field'}}\"\n                       placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\"\n                       autocomplete=\"off\"\n                >\n                <button class=\"quickSearch-btn\" type=\"submit\" aria-label=\"Submit Quick Search\">\n                  <svg aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg>\n                </button>\n            </div>\n        </fieldset>\n    </form>\n  </div>\n</div>\n<div class=\"container\">\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n\n    <p role=\"status\"\n       aria-live=\"polite\"\n       class=\"aria-description--hidden\"\n       data-search-aria-message-predefined-text=\"{{{lang 'search.results.quick_count_live'}}}\"\n    ></p>\n\n    {{#or (if theme_settings.quickSearch_Recommended_Products) (if theme_settings.quickSearch_topSearches)}}\n      <div class=\"quickSearch-recommended\">\n        {{#if theme_settings.quickSearch_Recommended_Products}}\n          <div class=\"qsRecommended-products\">\n            <h3 class=\"topSearches-title\">{{lang 'search.recommended_products_title'}}</h3>\n            <section class=\"productCarousel\">\n              {{> components/halothemes/products/halo-sample-product}}\n              {{> components/halothemes/products/halo-sample-product}}\n              {{> components/halothemes/products/halo-sample-product}}\n            </section>\n          </div>\n        {{/if}}\n        {{#if theme_settings.quickSearch_topSearches}}\n          <div class=\"qsRecommended-topSearches\">\n            <h3 class=\"topSearches-title\">{{lang 'search.top_searches'}}</h3>\n            <ul class=\"topSearches-list\">\n              <li>\n                <a class=\"topSearches-btn\">\n                  <span>{{theme_settings.quickSearch_topSearches_text1}}</span>\n                  <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg>\n                </a>\n              </li>\n              <li>\n                <a class=\"topSearches-btn\">\n                  <span>{{theme_settings.quickSearch_topSearches_text2}}</span>\n                  <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg>\n                </a>\n              </li>\n              <li>\n                <a class=\"topSearches-btn\">\n                  <span>{{theme_settings.quickSearch_topSearches_text3}}</span>\n                  <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg>\n                </a>\n              </li>\n              <li>\n                <a class=\"topSearches-btn\">\n                  <span>{{theme_settings.quickSearch_topSearches_text4}}</span>\n                  <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg>\n                </a>\n              </li>\n              <li>\n                <a class=\"topSearches-btn\">\n                  <span>{{theme_settings.quickSearch_topSearches_text5}}</span>\n                  <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg>\n                </a>\n              </li>\n              <li>\n                <a class=\"topSearches-btn\">\n                  <span>{{theme_settings.quickSearch_topSearches_text6}}</span>\n                  <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg>\n                </a>\n              </li>\n              <li>\n                <a class=\"topSearches-btn\">\n                  <span>{{theme_settings.quickSearch_topSearches_text7}}</span>\n                  <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg>\n                </a>\n              </li>\n            </ul>\n          </div>\n        {{/if}}\n      </div>\n    {{/or}}\n</div>\n",
  "components/common/modal/modal-close-btn": "<button class=\"modal-close\"\n        type=\"button\"\n        title=\"{{lang 'common.close'}}\"\n        aria-label=\"{{lang 'common.close'}}\"\n        {{additional_attr}}\n>\n    <span class=\"aria-description--hidden\">{{lang 'common.close'}}</span>\n    <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-close\"></svg>\n</button>\n",
  "components/halothemes/products/halo-sample-product": "<div data-product-slide class=\"productCarousel-slide productCarousel-sample\">\n\t<article class=\"card card-sample\">\n\t    <figure class=\"card-figure\">\n\t        <a href=\"javascript:void(0)\"\n\t           class=\"card-figure__link\"\n\t           aria-label=\"Sample Card\">\n\t            <div class=\"card-img-container animated-loading\"></div>\n\t        </a>\n\t    </figure>\n\t    <div class=\"card-body\">\n\t    \t{{#if theme_settings.halo_card_brand}}\n\t        \t<p class=\"card-text animated-loading\" data-test-info-type=\"brandName\"><span>Brands</span></p>\n\t        {{/if}}\n\n\t        <h3 class=\"card-title animated-loading\">\n\t            <a aria-label=\"Sample Card Title\" href=\"#\"><span>Sample Card</span></a>\n\t        </h3>\n\t        \n\t        {{#if theme_settings.halo_card_ratings}}\n\t            <p class=\"card-text animated-loading\" data-test-info-type=\"productRating\">\n\t                <span class=\"rating--small\">\n\t                    {{> components/products/ratings rating_target=name rating=rating}}\n\t                </span>\n\t            </p>\n            {{/if}}\n\n\t        <div class=\"card-text animated-loading\" data-test-info-type=\"price\">\n\t            <div class=\"price-section\">\n\t\t\t        <span class=\"price\">$000.00</span>\n\t\t\t    </div>\n\t        </div>\n\n\t        {{#if theme_settings.haloAddOptionForProduct}}\n\t            {{#unless add_to_cart_url}}\n\t                {{#or (if pre_order) (if has_options) }}\n\t                    <div class=\"card-option\">\n\t\t                    <div class=\"form-field\" data-product-attribute=\"swatch\">\n\t\t                    \t<label class=\"form-option form-option-swatch\">\n\t\t\t\t\t\t            <span class=\"form-option-variant form-option-variant--color\" title=\"Color Sample\" style=\"background-color: #f6f6f6\"></span>\n\t\t\t\t\t\t        </label>\n\t\t                    </div>\n\t\t                </div>\n\t                {{/or}}\n\t                {{#and (if out_of_stock_message) (if has_options) }}\n\t                    <div class=\"card-option\">\n\t\t                    <div class=\"form-field\" data-product-attribute=\"swatch\">\n\t\t                    \t<label class=\"form-option form-option-swatch\">\n\t\t\t\t\t\t            <span class=\"form-option-variant form-option-variant--color\" title=\"Color Sample\" style=\"background-color: #f6f6f6\"></span>\n\t\t\t\t\t\t        </label>\n\t\t                    </div>\n\t\t                </div>\n\t                {{/and}}\n\t            {{/unless}}\n\t        {{/if}}\n\t    </div>\n\t</article>\n</div>\n",
  "components/products/ratings": "<span class=\"product-ratings\" role=\"img\"\n    {{#if rating_target}}\n      aria-label=\"{{lang 'products.reviews.rating_aria_label' rating_target=rating_target current_rating=rating max_rating=5}}\"\n    {{else}}\n      aria-label=\"{{lang 'products.reviews.rating_aria_label' rating_target='Product' current_rating=rating max_rating=5}}\"\n    {{/if}}\n>\n    {{#for 1 5}}\n        {{#if ../this.rating '>=' $index}}\n            <span class=\"icon icon--ratingFull\">\n                {{#if ../theme_settings.halo_homepage_layout_3}}\n                    <svg>\n                        <use xlink:href=\"#icon-star-2\" />\n                    </svg>\n                {{else}}\n                    <svg>\n                        <use xlink:href=\"#icon-star\" />\n                    </svg>\n                {{/if}}\n            </span>\n        {{else}}\n            <span class=\"icon icon--ratingEmpty\">\n                {{#if ../theme_settings.halo_homepage_layout_3}}\n                    <svg>\n                        <use xlink:href=\"#icon-star-2\" />\n                    </svg>\n                {{else}}\n                    <svg>\n                        <use xlink:href=\"#icon-star\" />\n                    </svg>\n                {{/if}}\n            </span>\n        {{/if}}\n    {{/for}}\n</span>\n"
}
