{
  "components/halothemes/cart/configure-options/set-rectangle": "<div class=\"form-field\" data-product-attribute=\"set-rectangle\">\n    <label class=\"form-label form-label--inlineSmall\">\n        {{this.display_name}}:\n        {{> components/common/requireness-msg}}\n        <span data-option-value></span>\n    </label>\n    {{#each this.values}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"edit_attribute_rectangle_{{../id}}_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}\n                checked\n                data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}>\n        <label class=\"form-option form-option-rectangle\" for=\"edit_attribute_rectangle_{{../id}}_{{id}}\" data-product-attribute-value=\"{{id}}\">\n            {{#if this.label.length '>' 2}}\n                <span class=\"form-option-variant form-option-length\">{{this.label}}</span>\n            {{else}}\n                <span class=\"form-option-variant\">{{this.label}}</span>\n            {{/if}}\n        </label>\n    {{/each}}\n</div>\n",
  "components/common/requireness-msg": "<small>\n    {{#if required}}\n    \t{{lang 'common.required_dot'}}\n    {{else}}\n    \t{{lang 'common.optional'}}\n    {{/if}}\n</small>\n"
}
