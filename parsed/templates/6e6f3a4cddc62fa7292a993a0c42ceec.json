{
  "components/common/navigation-menu": "<nav class=\"navPages\">\n    <ul class=\"navPages-list{{#if theme_settings.haloMegamenu}} navPages-list-megamenu{{/if}}{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\n        {{#if theme_settings.menu_brands}}\n            <li class=\"navPages-item navPages-item-page\">\n                <a class=\"navPages-action{{#if page_type '==' 'brands'}} activePage{{/if}}\"\n                    href=\"/brands\"\n                    aria-label=\"Brands\"\n                >\n                    <span>{{lang 'brand.label'}}</span>\n                </a>\n            </li>\n        {{/if}}\n        {{#each categories}}\n            <li class=\"navPages-item{{#if children}} has-dropdown{{/if}}\">\n                {{#if ../theme_settings.navigation_design '===' 'simple'}}\n                    {{> components/common/navigation-list theme_settings = ../theme_settings}}\n                {{/if}}\n                {{#if ../theme_settings.navigation_design '===' 'alternate'}}\n                    {{> components/common/navigation-list-alternate theme_settings = ../theme_settings}}\n                {{/if}}\n            </li>\n        {{/each}}\n        {{#if theme_settings.hide_content_navigation}}\n            {{#each pages}}\n                {{#if name '==' 'Pages'}}\n                    <li class=\"navPages-item navPages-item-page{{#if children}} has-dropdown{{/if}}\">\n                        {{> components/common/navigation-list-alternate}}\n                    </li>\n                {{/if}}\n                {{#if name '==' 'Documentation'}}\n                    <li class=\"navPages-item navPages-item-page a{{#if children}} has-dropdown{{/if}}\">\n                        {{> components/common/navigation-list-alternate}}\n                    </li>\n                {{/if}}\n            {{/each}}\n        {{else}}\n            {{#each pages}}\n                <li class=\"navPages-item navPages-item-page{{#if children}} has-dropdown{{/if}}\">\n                    {{> components/common/navigation-list-alternate}}\n                </li>\n            {{/each}}\n        {{/if}}\n    </ul>\n    <ul class=\"navPages-list navPages-list--user\">\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action\" href=\"{{urls.account.wishlists.all}}\">\n                <svg class=\"icon\"><use xlink:href=\"#icon-wishlist\"/></svg>\n                <span class=\"text\">{{lang 'account.mobile_nav.wishlists'}}</span>\n            </a>\n        </li>\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action\" href=\"/contact-us/\">\n                <svg class=\"icon icon-location\"><use xlink:href=\"#icon-location\"/></svg>\n                <span class=\"text\">Store Locator</span>\n            </a>\n        </li>\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action\" href=\"tel:{{settings.phone_number}}\">\n                <svg class=\"icon icon-telephone\"><use xlink:href=\"#icon-phone\"/></svg>\n                <span class=\"text\">Contact us: {{settings.phone_number}}</span>\n            </a>\n        </li>\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--storeCredit\">\n                    <span>{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}</span>\n                </a>\n            </li>\n        {{/if}}\n        {{#if settings.gift_certificates_enabled}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\"\n                   href=\"{{urls.gift_certificate.purchase}}\"\n                   aria-label=\"{{lang 'common.gift_cert'}}\"\n                >\n                    <svg class=\"icon\"><use xlink:href=\"#icon-gift-mobile\"/></svg>\n                    <span>{{lang 'common.gift_cert'}}</span>\n                </a>\n            </li>\n        {{/if}}\n        {{#if currency_selector.currencies.length '>' 1}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-actionCurrency has-subMenu\"\n                   href=\"#\"\n                   data-collapsible=\"navPages-currency\"\n                   aria-controls=\"navPages-currency\"\n                   aria-expanded=\"false\"\n                   aria-label=\"{{lang 'common.currency' code=currency_selector.active_currency_code}}\"\n                >\n                    {{{currency_selector.active_currency_flag}}}\n                    <span class=\"cur-code\">{{currency_selector.active_currency_code}}</span>\n                    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n                        <svg>\n                            <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-down\"></use>\n                        </svg>\n                    </i>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        {{#each currency_selector.currencies}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{{switch_url}}}\"\n                               aria-label=\"{{name}}\"\n                               data-currency-code=\"{{{code}}}\"\n                               {{#if cart_currency_switch_url.length '>' 0 }}\n                               data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\n                               {{/if}}\n                               data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n                            >\n                                {{#if is_active}}\n                                    <strong>{{name}}</strong>\n                                {{else}}\n                                    {{name}}\n                                {{/if}}\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                </div>\n            </li>\n        {{/if}}\n        <li class=\"navPages-item\">\n            {{> components/common/social-links}}\n        </li>\n    </ul>\n</nav>\n",
  "components/common/navigation-list": "{{#if children}}\n<a class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}{{#if theme_settings.menu_sale '==' name }} menu-sale{{/if}}\"\n   href=\"{{url}}\"\n   data-collapsible=\"navPages-{{id}}\"\n>\n    {{name}}\n    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n        <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\n    </i>\n</a>\n<div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item\">\n            <a class=\"navPage-subMenu-action navPages-action\"\n               href=\"{{url}}\"\n               aria-label=\"{{lang 'category.view_all.name' category=name}}\"\n            >\n                {{lang 'category.view_all.name' category=name}}\n            </a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\n                       href=\"{{url}}\"\n                       aria-label=\"{{name}}\"\n                    >\n                        {{name}}\n                        <span class=\"collapsible-icon-wrapper\"\n                            data-collapsible=\"navPages-{{id}}\"\n                            data-collapsible-disabled-breakpoint=\"medium\"\n                            data-collapsible-disabled-state=\"open\"\n                            data-collapsible-enabled-state=\"closed\"\n                        >\n                            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n                                <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\n                            </i>\n                        </span>\n                    </a>\n                    <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\n                        {{#each children}}\n                        <li class=\"navPage-childList-item\">\n                            <a class=\"navPage-childList-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                               href=\"{{url}}\"\n                               aria-label=\"{{name}}\"\n                            >\n                                {{name}}\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                       href=\"{{url}}\"\n                       aria-label=\"{{name}}\"\n                    >\n                        {{name}}\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n{{else}}\n<a class=\"navPages-action{{#if is_active}} activePage{{/if}}{{#if theme_settings.menu_sale '==' name }} menu-sale{{/if}}\"\n   href=\"{{url}}\"\n   aria-label=\"{{name}}\"\n>\n    {{name}}\n</a>\n{{/if}}\n",
  "components/common/navigation-list-alternate": "{{#if children}}\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root{{#if is_active}} activePage{{/if}}{{#if theme_settings.menu_sale '==' name }} menu-sale{{/if}}\"\n       href=\"{{url}}\"\n       aria-label=\"{{name}}\"\n    >\n        <span>{{name}}</span>\n        <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n            <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\n        </i>\n    </a>\n    {{> components/common/navigation-dropdown id=id }}\n{{else}}\n    <a class=\"navPages-action{{#if is_active}} activePage{{/if}}{{#if theme_settings.menu_sale '==' name }} menu-sale{{/if}}\"\n       href=\"{{url}}\"\n       aria-label=\"{{name}}\"\n    >\n        <span>{{name}}</span>\n    </a>\n{{/if}}\n",
  "components/common/social-links": "{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\" aria-label=\"Open {{display_name}} in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                </a>\n            </li>\n        {{/each}}\n        {{#if theme_settings.social_icon_google_plus_link}}\n            <li class=\"socialLinks-item google-plus\">\n                <a class=\"icon icon--google-plus\" href=\"{{theme_settings.social_icon_google_plus_link}}\" target=\"_blank\" rel=\"noopener\" aria-label=\"Open Google Plus Social Media in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-google-plus\" /></svg>\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.social_icon_snapchat_link}}\n            <li class=\"socialLinks-item snapchat\">\n                <a class=\"icon icon--snapchat\" href=\"{{theme_settings.social_icon_snapchat_link}}\" target=\"_blank\" rel=\"noopener\" aria-label=\"Open Snapchat Social Media in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-snapchat-ghost\" /></svg>\n                </a>\n            </li>\n        {{/if}}\n    </ul>\n{{/if}}\n",
  "components/common/navigation-dropdown": "<div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-parent\">\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max\" href=\"{{url}}\"><span>{{lang 'category.view_all.name' category=name}}</span></a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-navId=\"navPages-{{id}}\">\n                        <span>{{name}}</span><i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                    </a>\n                    {{> components/common/navigation-dropdown}}\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"><span>{{name}}</span></a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n"
}
