{
  "components/halothemes/cart/configure-options/input-numbers": "<div class=\"form-field\" data-product-attribute=\"input-number\">\n\t<label class=\"form-label form-label--inlineSmall\" for=\"edit_attribute_number_{{id}}\">\n        {{ this.display_name }}:\n\n        {{> components/common/requireness-msg}}\n    </label>\n    <input class=\"form-input\" type=\"number\" id=\"edit_attribute_number_{{id}}\" {{#unless this.integer_only}}step=\"any\"{{/unless}} name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\n</div>\n",
  "components/common/requireness-msg": "<small>\n    {{#if required}}\n    \t{{lang 'common.required_dot'}}\n    {{else}}\n    \t{{lang 'common.optional'}}\n    {{/if}}\n</small>\n"
}
