{
  "components/common/navigation": "<nav class=\"navUser\">\n    {{#if theme_settings.halo_header_customTextLink}}\n        <ul class=\"navUser-section navUser-section--link\">\n            <li class=\"navUser-item navUser-item--contact\">\n                <a class=\"navUser-action navUserAction-contact\"\n                   href=\"{{theme_settings.halo_header_customLink_1}}\"\n                   aria-label=\"Contact\"\n                >\n                    {{theme_settings.halo_header_customText_1}}\n                </a>\n            </li>\n            <li class=\"navUser-item navUser-item--about\">\n                <a class=\"navUser-action navUserAction-about\"\n                   href=\"{{theme_settings.halo_header_customLink_2}}\"\n                   aria-label=\"About\"\n                >\n                    {{theme_settings.halo_header_customText_2}}\n                </a>\n            </li>\n        </ul>\n    {{/if}}\n    {{#unless theme_settings.halo_homepage_layout_4}}\n      {{> components/common/currency-selector}}\n    {{/unless}}\n\n    <ul class=\"navUser-section navUser-section--alt\">\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navUser-item\">\n                <a class=\"navUser-action navUser-action--storeCredit\"\n                   data-dropdown=\"storeCredit-dropdown\"\n                   data-options=\"align:bottom\"\n                   href=\"{{urls.cart}}\"\n                   aria-label=\"{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\"\n                >\n                    <span class=\"navUser-action-divider\">\n                        {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                    </span>\n                </a>\n                <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\n                    {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\n                </div>\n            </li>\n        {{/if}}\n        <li class=\"navUser-item navUser-itemBlock--compare\">\n            <a class=\"navUser-action navUser-item--compare\"\n               href=\"{{urls.compare}}\"\n               data-compare-nav\n               aria-label=\"{{lang 'common.compare'}}\"\n            >\n                <span class=\"navUser-compare--title\">{{lang 'common.compare'}}</span>\n                <span class=\"countPill countPill--positive countPill--alt\"></span>\n            </a>\n        </li>\n        {{#or theme_settings.halo_homepage_layout_1 theme_settings.halo_homepage_layout_2 }}\n            <li class=\"navUser-item navUser-item--account\">\n                {{#if customer}}\n                    <a class=\"navUser-action navUserAction-login\"\n                       href=\"{{urls.account.index}}\"\n                       aria-label=\"{{lang 'common.account'}}\"\n                    >\n                        {{lang 'common.account'}}\n                    </a>\n                {{else}}\n                    <a class=\"navUser-action navUserAction-login\"\n                       href=\"{{urls.auth.login}}\"\n                       aria-label=\"{{lang 'common.login'}}\"\n                    >\n                        {{lang 'common.login'}}\n                    </a>\n                {{/if}}\n            </li>\n        {{/or}}\n        <li class=\"navUser-item\">\n            <button class=\"navUser-action navUser-action--quickSearch quick-search-expand\"\n               type=\"button\"\n               id=\"quick-search-expand\"\n               data-search=\"quickSearch\"\n               aria-controls=\"quickSearch\"\n               aria-label=\"{{lang 'common.search'}}\"\n            >   \n                {{#if theme_settings.halo_homepage_layout_1}}\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg>\n                {{else if theme_settings.halo_homepage_layout_2}}\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-search2\"></use></svg>\n                {{else}}\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-search3\"></use></svg>\n                {{/if}}\n                <span>{{lang 'common.search'}}</span>\n            </button>\n        </li>\n        {{#or theme_settings.halo_homepage_layout_3 or theme_settings.halo_homepage_layout_4 }}\n            <li class=\"navUser-item navUser-item--account\">\n                {{#if customer}}\n                    <a class=\"navUser-action navUserAction-login\"\n                       href=\"{{urls.account.index}}\"\n                       aria-label=\"{{lang 'common.account'}}\"\n                    >\n                        <div class=\"navUser-icon\">\n                            <svg aria-hidden=\"true\"><use xlink:href=\"#icon-user-2\"></use></svg>\n                        </div>\n                    </a>\n                {{else}}\n                    <div class=\"navUser-item__item\">\n                        <a class=\"navUser-action navUserAction-login\"\n                           href=\"{{urls.auth.login}}\"\n                           aria-label=\"{{lang 'common.login'}}\"\n                        >\n                            <div class=\"navUser-icon\">\n                                <svg aria-hidden=\"true\"><use xlink:href=\"#icon-user-2\"></use></svg>\n                            </div>\n                        </a>\n                    </div>\n                {{/if}}\n            </li>\n            {{#if settings.show_wishlist}}\n                <li class=\"navUser-item navUser-item--wishlist\">\n                    <div class=\"navUser-item__item\">\n                        <a class=\"navUser-action\" \n                           href=\"{{urls.account.wishlists.all}}\"\n                           aria-label=\"{{lang 'account.wishlists.heading'}}\"\n                        >\n                            <div class=\"navUser-icon\">\n                                <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.wishlists.heading'}}\">\n                                    <title>{{lang 'account.wishlists.heading'}}</title>\n                                    <desc>{{lang 'account.wishlists.heading'}}</desc>\n                                    <use xlink:href=\"#icon-wishlist-2\"></use>\n                                </svg>\n                            </div>\n                        </a>\n                    </div>\n                </li>\n            {{/if}}\n        {{/or}}\n        <li class=\"navUser-item navUser-item--cart\">\n            <a class=\"navUser-action navUser-actionCart\"\n               data-cart-preview\n               data-options=\"align:right\"\n               href=\"{{urls.cart}}\"\n               aria-label=\"{{lang 'cart.nav_aria_label'}}\"\n            > \n            {{#if theme_settings.halo_homepage_layout_4 }}\n                <svg aria-hidden=\"true\"><use xlink:href=\"#icon-cart-2\"></use></svg>\n            {{ else }}\n                <svg aria-hidden=\"true\"><use xlink:href=\"#icon-cart\"></use></svg>\n            {{/if}}\n                <span class=\"navUser-item-cartLabel\">{{lang 'common.cart'}}</span>\n                <span class=\"countPill cart-quantity\"></span>\n            </a>\n        </li>\n    </ul>\n    {{#if theme_settings.halo_homepage_layout_4}}\n      {{> components/common/currency-selector}}\n    {{/if}}\n</nav>\n",
  "components/common/currency-selector": "{{#if currency_selector.currencies.length '>' 1}}\n<ul class=\"navUser-section\">\n    <li class=\"navUser-item navUser-item--currency\">\n        <a class=\"navUser-action navUser-action--currencySelector has-dropdown\"\n           href=\"#\"\n           data-dropdown=\"currencySelection\"\n           aria-controls=\"currencySelection\"\n           aria-expanded=\"false\"\n           aria-label=\"{{lang 'common.currency' code=currency_selector.active_currency_code}}\"\n        >\n            <span class=\"cur-flag\">{{{currency_selector.active_currency_flag}}}</span>\n            <span class=\"cur-code\">{{currency_selector.active_currency_code}}</span>\n            <i class=\"icon\" aria-hidden=\"true\">\n                <svg>\n                    <use xlink:href=\"#icon-chevron-down\" />\n                </svg>\n            </i>\n        </a>\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n            {{#each currency_selector.currencies}}\n            <li class=\"dropdown-menu-item\">\n                <a href=\"{{{switch_url}}}\"\n                   data-currency-code=\"{{{code}}}\"\n                   {{#if cart_currency_switch_url.length '>' 0 }}\n                   data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\n                   {{/if}}\n                   data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n                >\n                {{#if is_active}}\n                    {{{flag}}} <strong>{{code}}</strong>\n                {{else}}\n                    {{{flag}}} <span>{{code}}</span>\n                {{/if}}\n                </a>\n            </li>\n            {{/each}}\n        </ul>\n    </li>\n</ul>\n{{/if}}\n"
}
