{
  "components/halothemes/global/halo-ask-an-expert-form": "<input type=\"hidden\" name=\"page_id\" id=\"page_id\" value=\"{{forms.contact.page_id}}\">\n{{#if forms.contact.name}}\n    <div class=\"form-field\">\n        <input class=\"form-input\" type=\"text\" id=\"contact_fullname\" name=\"contact_fullname\" value=\"{{customer.name}}\" placeholder=\"{{lang 'forms.contact_us.full_name'}}\">\n    </div>\n{{/if}}\n\n{{#if forms.contact.phone}}\n    <div class=\"form-field\">\n        <input class=\"form-input\" type=\"text\" id=\"contact_phone\" name=\"contact_phone\" value=\"{{customer.phone}}\" placeholder=\"{{lang 'forms.contact_us.phone'}}\">\n    </div>\n{{/if}}\n\n<div class=\"form-field\">\n    <input class=\"form-input\" type=\"text\" id=\"contact_email\" name=\"contact_email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'forms.contact_us.email'}}\" required>\n</div>\n\n{{#if forms.contact.order}}\n    <div class=\"form-field\">\n        <input class=\"form-input\" type=\"text\" id=\"contact_orderno\" name=\"contact_orderno\" placeholder=\"{{lang 'forms.contact_us.order'}}\">\n    </div>\n{{/if}}\n\n{{#if forms.contact.company}}\n    <div class=\"form-field\">\n        <input class=\"form-input\" type=\"text\" id=\"contact_companyname\" name=\"contact_companyname\" placeholder=\"{{lang 'forms.contact_us.company'}}\">\n    </div>\n{{/if}}\n\n{{#if forms.contact.rma}}\n    <div class=\"form-field\">\n        <input class=\"form-input\" type=\"text\" id=\"contact_rma\" name=\"contact_rma\" placeholder=\"{{lang 'forms.contact_us.rma'}}\">\n    </div>\n{{/if}}\n"
}
