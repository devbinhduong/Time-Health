{
  "components/halothemes/products/halo-sample-product": "<div data-product-slide class=\"productCarousel-slide productCarousel-sample\">\n\t<article class=\"card card-sample\">\n\t    <figure class=\"card-figure\">\n\t        <a href=\"javascript:void(0)\"\n\t           class=\"card-figure__link\"\n\t           aria-label=\"Sample Card\">\n\t            <div class=\"card-img-container animated-loading\"></div>\n\t        </a>\n\t    </figure>\n\t    <div class=\"card-body\">\n\t    \t{{#if theme_settings.halo_card_brand}}\n\t        \t<p class=\"card-text animated-loading\" data-test-info-type=\"brandName\"><span>Brands</span></p>\n\t        {{/if}}\n\n\t        <h3 class=\"card-title animated-loading\">\n\t            <a aria-label=\"Sample Card Title\" href=\"#\"><span>Sample Card</span></a>\n\t        </h3>\n\t        \n\t        {{#if theme_settings.halo_card_ratings}}\n\t            <p class=\"card-text animated-loading\" data-test-info-type=\"productRating\">\n\t                <span class=\"rating--small\">\n\t                    {{> components/products/ratings rating_target=name rating=rating}}\n\t                </span>\n\t            </p>\n            {{/if}}\n\n\t        <div class=\"card-text animated-loading\" data-test-info-type=\"price\">\n\t            <div class=\"price-section\">\n\t\t\t        <span class=\"price\">$000.00</span>\n\t\t\t    </div>\n\t        </div>\n\n\t        {{#if theme_settings.haloAddOptionForProduct}}\n\t            {{#unless add_to_cart_url}}\n\t                {{#or (if pre_order) (if has_options) }}\n\t                    <div class=\"card-option\">\n\t\t                    <div class=\"form-field\" data-product-attribute=\"swatch\">\n\t\t                    \t<label class=\"form-option form-option-swatch\">\n\t\t\t\t\t\t            <span class=\"form-option-variant form-option-variant--color\" title=\"Color Sample\" style=\"background-color: #f6f6f6\"></span>\n\t\t\t\t\t\t        </label>\n\t\t                    </div>\n\t\t                </div>\n\t                {{/or}}\n\t                {{#and (if out_of_stock_message) (if has_options) }}\n\t                    <div class=\"card-option\">\n\t\t                    <div class=\"form-field\" data-product-attribute=\"swatch\">\n\t\t                    \t<label class=\"form-option form-option-swatch\">\n\t\t\t\t\t\t            <span class=\"form-option-variant form-option-variant--color\" title=\"Color Sample\" style=\"background-color: #f6f6f6\"></span>\n\t\t\t\t\t\t        </label>\n\t\t                    </div>\n\t\t                </div>\n\t                {{/and}}\n\t            {{/unless}}\n\t        {{/if}}\n\t    </div>\n\t</article>\n</div>\n",
  "components/products/ratings": "<span class=\"product-ratings\" role=\"img\"\n    {{#if rating_target}}\n      aria-label=\"{{lang 'products.reviews.rating_aria_label' rating_target=rating_target current_rating=rating max_rating=5}}\"\n    {{else}}\n      aria-label=\"{{lang 'products.reviews.rating_aria_label' rating_target='Product' current_rating=rating max_rating=5}}\"\n    {{/if}}\n>\n    {{#for 1 5}}\n        {{#if ../this.rating '>=' $index}}\n            <span class=\"icon icon--ratingFull\">\n                {{#if ../theme_settings.halo_homepage_layout_3}}\n                    <svg>\n                        <use xlink:href=\"#icon-star-2\" />\n                    </svg>\n                {{else}}\n                    <svg>\n                        <use xlink:href=\"#icon-star\" />\n                    </svg>\n                {{/if}}\n            </span>\n        {{else}}\n            <span class=\"icon icon--ratingEmpty\">\n                {{#if ../theme_settings.halo_homepage_layout_3}}\n                    <svg>\n                        <use xlink:href=\"#icon-star-2\" />\n                    </svg>\n                {{else}}\n                    <svg>\n                        <use xlink:href=\"#icon-star\" />\n                    </svg>\n                {{/if}}\n            </span>\n        {{/if}}\n    {{/for}}\n</span>\n"
}
