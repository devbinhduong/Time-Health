{
  "components/halothemes/category/halo-category-toolbar": "<div class=\"halo-category-toolbar\">\n    <div class=\"column-left\">\n        <div class=\"sidebar-label\">\n            {{#if show_sidebar}}\n                <a href=\"javascript:void(0)\" class=\"categorySidebar-btn\" data-page-type=\"{{page_type}}\">\n                    <span class=\"icon-filter\">\n                        <svg class=\"icon\" aria-label=\"Button Show Sidebar\"><use xlink:href=\"#icon-filter\"></use></svg>\n                    </span>\n                    <span class=\"sidebar_more\">{{lang 'common.show_sidebar'}}</span>\n                </a>\n            {{/if}}\n        </div>\n    </div>\n    <div class=\"column-right\">\n        {{#or (if category.products) (if product_results.products.length) (if page_type '==' 'brand')}}\n            {{#if theme_settings.haloToolbar_viewAs}}\n                <div class=\"view-as-btn\">\n                    <label class=\"form-label\">{{lang 'products.view_as'}}</label>\n                    <div class=\"btn-group\">\n                        <div class=\"btn-select\">\n                            <a href=\"javascript:void(0);\" id=\"grid-view\" title=\"Grid View\" class=\"btn-view grid-view{{#if theme_settings.product_list_display_mode '===' 'grid'}} current-view{{/if}}\">\n                                <div class=\"icon-view icon-view-3\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                                <div class=\"icon-view icon-view-3\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                                <div class=\"icon-view icon-view-3\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                            </a>\n                            <a href=\"javascript:void(0);\" id=\"list-view\" title=\"List View\" class=\"btn-view list-view{{#if theme_settings.product_list_display_mode '===' 'list'}} current-view{{/if}}\">\n                                <div class=\"icon-view\">\n                                    <div class=\"icon-bar\"></div>\n                                    <div class=\"icon-bar\"></div>\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                            </a>\n                        </div>\n                        <div class=\"btn-dropdown{{#if theme_settings.show_sidebar '==' 'show'}} sidebar-show{{else}} sidebar-hide{{/if}}\">\n                            <a href=\"javascript:void(0);\" id=\"grid-view2\" data-grid=\"2\" title=\"Grid View\" class=\"btn-view grid-view grid-view2{{#if theme_settings.product_list_display_mode '===' 'grid'}}{{#if theme_settings.product_grid_view_column '==' 2}} current-view{{/if}}{{/if}}\">\n                                <div class=\"icon-view icon-view-2\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                                <div class=\"icon-view icon-view-2\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                            </a>\n                            <a href=\"javascript:void(0);\" id=\"grid-view3\" data-grid=\"3\" title=\"Grid View\" class=\"btn-view grid-view grid-view3{{#if theme_settings.product_list_display_mode '===' 'grid'}}{{#if theme_settings.product_grid_view_column '==' 3}} current-view{{/if}}{{/if}}\">\n                                <div class=\"icon-view icon-view-3\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                                <div class=\"icon-view icon-view-3\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                                <div class=\"icon-view icon-view-3\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                            </a>\n                            <a href=\"javascript:void(0);\" id=\"grid-view4\" data-grid=\"4\" title=\"Grid View\" class=\"btn-view grid-view grid-view4{{#if theme_settings.product_list_display_mode '===' 'grid'}}{{#if theme_settings.product_grid_view_column '==' 4}} current-view{{/if}}{{/if}}\">\n                                <div class=\"icon-view icon-view-4\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                                <div class=\"icon-view icon-view-4\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                                <div class=\"icon-view icon-view-4\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                                <div class=\"icon-view icon-view-4\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                            </a>\n                            <a href=\"javascript:void(0);\" id=\"grid-view5\" data-grid=\"5\" title=\"Grid View\" class=\"btn-view grid-view grid-view5{{#if theme_settings.product_list_display_mode '===' 'grid'}}{{#if theme_settings.product_grid_view_column '==' 5}} current-view{{/if}}{{/if}}\">\n                                <div class=\"icon-view icon-view-5\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                                <div class=\"icon-view icon-view-5\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                                <div class=\"icon-view icon-view-5\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                                <div class=\"icon-view icon-view-5\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                                <div class=\"icon-view icon-view-5\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                            </a>\n                            <a href=\"javascript:void(0);\" id=\"grid-view6\" data-grid=\"6\" title=\"Grid View\" class=\"btn-view grid-view grid-view6{{#if theme_settings.product_list_display_mode '===' 'grid'}}{{#if theme_settings.product_grid_view_column '==' 6}} current-view{{/if}}{{/if}}\">\n                                <div class=\"icon-view icon-view-6\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                                <div class=\"icon-view icon-view-6\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                                <div class=\"icon-view icon-view-6\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                                <div class=\"icon-view icon-view-6\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                                <div class=\"icon-view icon-view-6\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                                <div class=\"icon-view icon-view-6\">\n                                    <div class=\"icon-bar\"></div>\n                                </div>\n                            </a>\n                        </div>\n                    </div>\n                </div>\n            {{/if}}\n        {{/or}}\n        {{#if category.products}}\n            {{#if theme_settings.haloToolbar_limit}}\n                <form class=\"actionBar show-product\" method=\"get\" data-sort-by data-show-products>\n                    {{> components/halothemes/category/halo-show-product limit=limit products_per_page=theme_settings.categorypage_products_per_page}}\n                </form>\n            {{/if}}\n            {{#if theme_settings.haloToolbar_sort}}\n                {{> components/products/filter sort=pagination.category.sort}}\n            {{/if}}\n        {{/if}}\n        {{#if product_results.products.length}}\n            {{#if theme_settings.haloToolbar_limit}}\n                <form class=\"actionBar show-product\" method=\"get\" data-sort-by data-show-products>\n                    {{> components/halothemes/category/halo-show-product limit=limit products_per_page=theme_settings.searchpage_products_per_page}}\n                </form>\n            {{/if}}\n            {{#if theme_settings.haloToolbar_sort}}\n                {{> components/products/filter sort=pagination.product_results.sort}}\n            {{/if}}\n        {{/if}}\n        {{#if page_type '==' 'brand'}}\n            {{#if theme_settings.haloToolbar_limit}}\n                <form class=\"actionBar show-product\" method=\"get\" data-sort-by data-show-products>\n                    {{> components/halothemes/category/halo-show-product limit=limit products_per_page=theme_settings.brandpage_products_per_page}}\n                </form>\n            {{/if}}\n            {{#if theme_settings.haloToolbar_sort}}\n                {{> components/products/filter sort=pagination.brand.sort}}\n            {{/if}}\n        {{/if}}\n    </div>\n</div>\n",
  "components/halothemes/category/halo-show-product": "<fieldset class=\"form-fieldset actionBar-section\">\n    <div class=\"form-field\">\n        <label class=\"form-label\" for=\"limit\">{{lang 'common.number_products.show'}}</label>\n        <select class=\"form-select form-select--small\" name=\"limit\" id=\"limit\">\n            <option value=\"8\"{{#if '8' '==' products_per_page}} selected{{/if}}>{{{lang 'common.number_products.show_text' number=8}}}</option>\n            <option value=\"12\"{{#if '12' '==' products_per_page}} selected{{/if}}>{{{lang 'common.number_products.show_text' number=12}}}</option>\n            <option value=\"20\"{{#if '20' '==' products_per_page}} selected{{/if}}>{{{lang 'common.number_products.show_text' number=20}}}</option>\n            <option value=\"32\"{{#if '32' '==' products_per_page}} selected{{/if}}>{{{lang 'common.number_products.show_text' number=32}}}</option>\n            <option value=\"60\"{{#if '60' '==' products_per_page}} selected{{/if}}>{{{lang 'common.number_products.show_text' number=60}}}</option>\n        </select>\n    </div>\n</fieldset>\n",
  "components/products/filter": "<form class=\"actionBar\" method=\"get\" data-sort-by=\"product\">\n    {{> components/category/sort-box sort=sort}}\n</form>\n",
  "components/category/sort-box": "<fieldset class=\"form-fieldset actionBar-section\">\n    <div class=\"form-field\">\n        <label class=\"form-label\" for=\"sort\">{{lang 'common.sorter.sort_by'}}</label>\n        <select class=\"form-select form-select--small \" name=\"sort\" id=\"sort\" role=\"listbox\">\n            <option value=\"featured\" {{#if sort '==' 'featured'}}selected{{/if}}>{{lang 'common.sorter.featured'}}</option>\n            <option value=\"newest\" {{#if sort '==' 'newest'}}selected{{/if}}>{{lang 'common.sorter.newest'}}</option>\n            <option value=\"bestselling\" {{#if sort '==' 'bestselling'}}selected{{/if}}>{{lang 'common.sorter.top_sellers'}}</option>\n            <option value=\"alphaasc\" {{#if sort '==' 'alphaasc'}}selected{{/if}}>{{lang 'common.sorter.alpha_asc'}}</option>\n            <option value=\"alphadesc\" {{#if sort '==' 'alphadesc'}}selected{{/if}}>{{lang 'common.sorter.alpha_desc'}}</option>\n            <option value=\"avgcustomerreview\" {{#if sort '==' 'avgcustomerreview'}}selected{{/if}}>{{lang 'common.sorter.reviews'}}</option>\n            <option value=\"priceasc\" {{#if sort '==' 'priceasc'}}selected{{/if}}>{{lang 'common.sorter.price_asc'}}</option>\n            <option value=\"pricedesc\" {{#if sort '==' 'pricedesc'}}selected{{/if}}>{{lang 'common.sorter.price_desc'}}</option>\n            {{#if result_count}}\n                <option value=\"relevance\" {{#if sort '==' 'relevance'}}selected{{/if}}>{{lang 'common.sorter.relevance'}}</option>\n            {{/if}}\n        </select>\n    </div>\n</fieldset>\n"
}
